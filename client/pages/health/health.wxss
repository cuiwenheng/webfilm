page{
	background-color:#EDEBE9;
	padding:15px 2% 5px 2%;
}
.page{
	width:96%;
}
.header{
	display:block;
	line-height: 30px;
	font-size:20px;
	color: #666;
	font-weight:bold;
	margin-bottom:8px;
}
.title{
	margin-left:10px;
	display:inline;
    padding-bottom: 2px;
    border-bottom: 2px solid #48C2F9;
}
.content{
	width:100%;
