	font-weight:bold;
}
.subtitle text{
	font-size:12px;
}
.weightbiao{
	position:relative;
	height:80%;
	width:100%;
	overflow:hidden;
}
.weightList-item{
	position: absolute;
 	left: 0;
 	top: 0;
 	width: 100%;
 	height: 100%;
 	z-index:100;
}
.weightList-zhizhen{
	position: absolute;
	top:10px;
	left:50%;
	background-color: red;
	margin-left:-1px;
	width: 2px;
	height:40px;
}
.weightList-sanjiao{
	position: absolute;
	top:40px;
	left:50%;
	width: 0;
	height: 0;
	margin-left:-5px;
	border-left: 5px solid transparent;  
	border-right: 5px solid transparent;
	border-bottom: 10px solid #ff0066;
}
.weightList-kedu{
	-webkit-transform-origin: center 120px;
	transform-origin: center 120px;
	position: relative;
 	left: 0;
 	top: 0;
 	width: 300%;
 	height: 300%;
 	margin-left:-100%;
}
.weightList-circle-item {
	-webkit-transform-origin: center 110px;
	transform-origin: center 110px;
	position: absolute;
	top:10px;
	left:50%;
	background-color: #000000;
	margin-left:-0.5px;
	width: 1px;
}
.shortitem{
	height:10px;
}
.longitem{
	height:15px;
}
.weightList-circle-text{
	-webkit-transform-origin: center 95px;
	transform-origin: center 95px;
	position: absolute;
	top:25px;
	left:50%;
	margin-left:-10px;
	margin-top:0px;
	width: 20px;
	height:10px;
	font-size:10px;
	text-align:center;
}
.weightsize{
