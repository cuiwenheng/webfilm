page{
	display: flex;
	flex-direction: column;
	background-color:#EDEBE9;
	box-sizing:border-box;
	padding:30rpx 30rpx 30rpx 30rpx;
}
.page{
	box-sizing:border-box;
	width:100%;
	heigth:100%;
	position:relative;
	background-color:#fff;
	padding-left:56rpx;
	padding-right:56rpx;
}

.header{
	position:relative;
	margin-top:144rpx;
}
.bigtitle {
	color:rgb(56,211,159);
	text-align: center;
	font-size: 48rpx;
	font-family:PingFangSC-Medium;
	font-weight:blod;
}
.littletitle {
	color:rgb(102,102,102);
	text-align: center;
	font-size: 24rpx;
	font-family:PingFangSC-Regular;
	font-weight:blod;
}
.content{
	position:relative;
	margin-top:176rpx;
}
.imgcenter{
	width:100%;
	text-align:center;
}
image {
	margin: 0 auto;
}
.finishview{
	position:relative;
	margin-top:150rpx;
	width:100%;
	height:60rpx;
	text-align:center;
	margin-bottom:120rpx;
}
.finishbutton{
	position:relative;
	margin:0 auto;
	background-color:rgb(0,211,66);
	text-align:center;
	font-size: 30rpx;
	line-height:60rpx;
	font-family:PingFangSC-Regular;
	font-weight:blod;
	color:rgb(255,255,255);
	width:200rpx;
	height:60rpx;
	border-radius:30rpx;
}
