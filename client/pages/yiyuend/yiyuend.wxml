<view class="page">
    <view class="content">
        <view class="imgcenter">
            <image src="{{host}}/resource/image/{{photo}}.png" style="width:{{image.autowidth}}rpx;height:{{image.autoheight}}rpx" bindload="autoimage" />
        </view>
    </view>
    <view class="header">
        <view class="bigtitle">
            {{title1}}
        </view>
        <view class="littletitle">
            {{title2}}
        </view>
    </view>
    <view class="finishview">
        <view class="finishbutton" bindtap="goAhead">完成</view>
    </view>
</view>