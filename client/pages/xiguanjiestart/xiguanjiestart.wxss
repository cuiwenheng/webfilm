
page{
	display: flex;
	flex-direction: column;
	background-color:#EDEBE9;
	box-sizing:border-box;
	padding:30rpx 30rpx 30rpx 30rpx;
}
.page{
	box-sizing:border-box;
	/*width:100%;*/
	position:relative;
	background-color:#fff;
	padding:10rpx;
}

.content{
	/*width:100%;*/
	margin-top:6rpx;
	border-radius:20rpx;
	overflow:hidden;
	background-color:#95BB57;
	padding: 40rpx;
}
.title{
	font-size: 36rpx;
	text-align: center;
}
.neirong{
	font-size: 32rpx;
	text-indent: 64rpx;
}
.timu{
	padding: 40rpx 0 0 0;
	font-size: 30rpx;
	text-indent: 30rpx;
}
.jieshao{
	font-size: 30rpx;
	text-indent: 30rpx;
	font-weight: bold;
}

.startview{
	position:relative;
	margin-top:40rpx;
	width:100%;
	height:60rpx;
	text-align:center;
	margin-bottom:40rpx;
}
.startbutton{
	position:relative;
	margin:0 auto;
	background-color:rgb(0,161,232);
	text-align:center;
	font-size: 30rpx;
	line-height:60rpx;
	font-family:PingFangSC-Regular;
	font-weight:blod;
	color:rgb(255,255,255);
	width:200rpx;
	height:60rpx;
	border-radius:30rpx;
}

